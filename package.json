{
  "name": "coutainer_wallet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/index.ts | cat",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "test": "echo \"No tests yet\" && exit 0",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/db/data-source.ts",
    "db:migrate": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:generate": "npm run typeorm migration:generate -- -n",
    "db:create": "npm run typeorm migration:create -- -n",
    "db:show": "npm run typeorm migration:show",
    "db:status": "npm run typeorm migration:show",
    "db:sync": "ts-node -e \"require('./dist');\"",
    "db:init": "npm run build && npm run db:migrate",
    "migrate": "node scripts/migrate.js",
    "migrate:prod": "NODE_ENV=production node scripts/migrate.js",
    "swagger:validate": "npx swagger-jsdoc --definition src/index.ts --apis src/routes/*.ts --dereference",
    "swagger:check": "npm run build && npm run swagger:validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coutainer/coutainer_wallet.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Coutainer/coutainer_wallet/issues"
  },
  "homepage": "https://github.com/Coutainer/coutainer_wallet#readme",
  "dependencies": {
    "@mysten/bcs": "^1.8.0",
    "@mysten/sui": "^1.38.0",
    "@mysten/sui.js": "^0.54.1",
    "@types/cors": "^2.8.19",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "bip39": "^3.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
